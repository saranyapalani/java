public class Main
{
    public static void main(String[] args) 
    {
       double l=8.5,b=5.6;
        double area, perimeter;
        area = l*b;
        perimeter =2*(l+b);
        System.out.println("Area of rectangle:" +area);
        System.out.println("\nPerimeter of rectangle:"+perimeter);
    }            
}
