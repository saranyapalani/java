import java.util.Scanner;

public class Main{
    
 public static void main(String[] args) {
		int amount = 0;
		for(int l = 1; l <= 4; l++){
			for(int j = 1; j <= 4; j++){
				for(int k = 1; k <= 4; k++){
					if(k != l && k != j && l != j){
						amount++;
						System.out.println(l + "" + j + "" + k);
					}
				}
			}
		}
		System.out.println("Total number of the three-digit-number is " + amount);
	}
}

