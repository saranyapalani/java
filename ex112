import java.util.Scanner;
public class Main {
     public static void main(String[] arg) 
	 {

		Scanner in = new Scanner(System.in);	
		System.out.print("Input a number: ");
	    int m = in.nextInt(); 
        int m1 = m;
		long ctr = 0;
		while (m != 0) 
		{
			ctr += m / 5;
			m /= 5;
		}
		System.out.printf("Number of trailing zeros of the factorial %d is %d ",m1,ctr);
		System.out.printf("\n");      	   
	}	
}
