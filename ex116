import java.util.*; 
public class Main {
 public static void main(String[] args)
 {
        for (int a = 1; a <= 100; a++) {
            if (a % 3 == 0 && a % 5 == 0) {
                System.out.printf("\n%d: fizz buzz",a);
            } else if (a % 5 == 0) {
                System.out.printf("\n%d: buzz",a);
            } else if (a % 3 == 0) {
                System.out.printf("\n%d: fizz",a);
            } 
        }
        System.out.printf("\n");
    }
}
